   "source": [
    "Nous présentons ici le type de modèle que nous avons utilisé. Il s'agit d'une version simplifiée d'un réseau de neurones de type Unet, avec des couches successives de convolution (expliquer le time embedding). "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "35ad792c",
   "metadata": {},
   "outputs": [],
   "source": [
    "class TimeEmbedding(nn.Module):\n",
    "    def __init__(self, dim):\n",
    "        super().__init__()\n",
    "        self.dim = dim\n",
    "\n",
    "    def forward(self, t):\n",
    "        half = self.dim // 2\n",
    "        emb = math.log(10000) / (half - 1)\n",
    "        emb = torch.exp(torch.arange(half, device=t.device) * -emb)\n",
    "        emb = t[:, None] * emb[None, :]\n",
    "        return torch.cat([torch.sin(emb), torch.cos(emb)], dim=1)\n",
    "\n",
    "\n",
    "class SimpleUNet(nn.Module):\n",
    "    def __init__(self):\n",
    "        super().__init__()\n",
    "\n",
    "        self.time_mlp = nn.Sequential(\n",
    "            TimeEmbedding(64),\n",
    "            nn.Linear(64, 64),\n",
    "            nn.ReLU()\n",
    "        )\n",
    "\n",
    "        self.conv1 = nn.Conv2d(3, 64, 3, padding=1)\n",
    "        self.conv2 = nn.Conv2d(64, 128, 3, padding=1)\n",
    "\n",
    "        self.conv7 = nn.Conv2d(128, 64, 3, padding=1)\n",
    "        self.conv8 = nn.Conv2d(64, 3, 3, padding=1)\n",
    "\n",
    "    def forward(self, x, t):\n",
    "        t = self.time_mlp(t).view(-1, 64, 1, 1)\n",
    "\n",
    "        h1 = F.relu(self.conv1(x) + t)\n",
    "        h2 = F.relu(self.conv2(h1))\n",
    "\n",
    "\n",
    "        h7 = F.relu(self.conv7(h2))\n",
    "        out = self.conv8(h7)\n",
    "\n",
    "        return out\n",
    "    \n",
    "\n",
    "class SimpleUNet_256(nn.Module):\n",
    "    def __init__(self):\n",
    "        super().__init__()\n",
    "\n",
    "        self.time_mlp = nn.Sequential(\n",
    "            TimeEmbedding(64),\n",
    "            nn.Linear(64, 64),\n",
    "            nn.ReLU()\n",
    "        )\n",
    "\n",
    "        self.conv1 = nn.Conv2d(3, 64, 3, padding=1)\n",
    "        self.conv2 = nn.Conv2d(64, 128, 3, padding=1)\n",
    "        self.conv3 = nn.Conv2d(128, 256, 3, padding=1)\n",
    "\n",
    "\n",
    "        self.conv6 = nn.Conv2d(256, 128, 3, padding=1)\n",
    "        self.conv7 = nn.Conv2d(128, 64, 3, padding=1)\n",
    "        self.conv8 = nn.Conv2d(64, 3, 3, padding=1)\n",
    "\n",
    "    def forward(self, x, t):\n",
    "        t = self.time_mlp(t).view(-1, 64, 1, 1)\n",
    "\n",
    "        h1 = F.relu(self.conv1(x) + t)\n",
    "        h2 = F.relu(self.conv2(h1))\n",
    "        h3 = F.relu(self.conv3(h2))\n",
    "\n",
    "        h6 = F.relu(self.conv6(h3))\n",
    "        h7 = F.relu(self.conv7(h6))\n",
    "        out = self.conv8(h7)\n",
    "\n",
    "        return out"
   ]
  },
