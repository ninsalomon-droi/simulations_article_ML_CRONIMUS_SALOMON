    "betas = torch.linspace(1e-4, 0.02, 200).to(device)\n",
    "alphas = 1 - betas\n",
    "alphas_bar = torch.cumprod(alphas, 0)\n",
    "\n",
    "@torch.no_grad()\n",
    "def dps_algo(y, model, steps=200, step_size=0.002):\n",
    "    x = torch.randn_like(y)\n",
    "\n",
    "    for t in reversed(range(steps)):\n",
    "        tt = torch.full((1,), t, device=device, dtype=torch.long)\n",
    "\n",
    "        eps = model(x, tt)\n",
    "        a_bar = alphas_bar[t]\n",
    "\n",
    "        # Estimation de x0\n",
    "        x0_hat = (x - torch.sqrt(1 - a_bar) * eps) / torch.sqrt(a_bar)\n",
    "\n",
    "        # Gradient du terme de vraisemblance\n",
    "        grad = (A_phase(x0_hat) - y) / (sigma ** 2)\n",
    "\n",
    "        # DPS update (sur x0 estimé)\n",
    "        x0_hat = x0_hat - step_size * grad\n",
    "\n",
    "        # Re-projection vers x_t\n",
    "        x = (\n",
    "            torch.sqrt(a_bar) * x0_hat +\n",
    "            torch.sqrt(1 - a_bar) * torch.randn_like(x)\n",
    "        )\n",
    "\n",
    "    return x"
   ]
